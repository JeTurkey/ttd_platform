<%- include('partials/header') -%>

<div id='main' class="col-md-9 col-lg-10 d-md-block" style="padding-left: 1.5rem; padding-right: 1.5rem;">
    <div class="row">
        政府部分分区
    </div>

    <% for(var i = 0; i < data.length; i+=3){ %>
        <div class="row">
            <% for(var j = 0; j <= (data.length - i) % 3; j++){ %>
                <% try{ %><div class="col-4"> <a href="/governmentTracker/<%= data[i+j].gov_dept_id %>"><%= data[i + j].gov_dept_name %></a></div><% }catch(err){}%>
            <% }%>
            
        </div>
    <% } %>


</div>

<%- include('partials/footer') -%>